[merge]
    tool = winmerge
[mergetool "winmerge"]
    path = \"C:\\Program Files\\WinMerge\\WinMergeU.exe\"
    cmd  = \"C:\\Program Files\\WinMerge\\WinMergeU.exe\" /m /r=\"$MERGED\" \"$LOCAL\" \"$BASE\" \"$REMOTE\"
[diff]
    tool = winmerge
    guitool = winmerge
[difftool "winmerge"]
    path = \"C:\\Program Files\\WinMerge\\WinMergeU.exe\"
    cmd  = \"C:\\Program Files\\WinMerge\\WinMergeU.exe\" -e -u -r -ub -dl \"Base\" -dr \"Mine\" \"$LOCAL\" \"$REMOTE\"
